#include <stdio.h>

int main() {
	int n, aux, primo = 1;
	
while((n/2)<=0){
	printf("Digite um numero inteiro: ");
	scanf("%d", &n);
	
	for (aux = 2; aux <= n/2; aux++)
	
	if ((n % aux) == 0)
	primo = 0;
	
	if (primo)
	printf("Numero primo\n");
	
	else
	printf("Não é um numero primo\n");
}
return 0;
}
